namespace ShopBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * Class Shop
 * @package ShopBundle\Entity
 * @ORM\Table(name="shop")
 * @ORM\Entity
 */

class Shop
{

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * @ORM\Column(type="string")
     */
    private $name;

    /**
     * @ORM\Column(type="string")
     */
    private $category;

}